[ClassicUI]
# 状态栏 X 位置
#MainWindowOffsetX=500
# 状态栏 Y 位置
#MainWindowOffsetY=40
# 字体大小
FontSize=12
# 字体
Font=文泉驿微米黑 Regular Regular Regular Regular Regular Regular Regular Regular Regular
# 菜单字体
MenuFont=文泉驿微米黑 Regular Regular Regular Regular Regular Regular Regular Regular Regular
# 字体区域
#FontLocale=zh_CN.UTF-8
# 使用系统托盘
# 可选值：
# True False
#UseTray=True
# 状态栏隐藏模式
# 可选值：
# Show
# Auto
# Hide when tray icon is available
# Hide
#MainWindowHideMode=Hide when tray icon is available
# 竖排候选词列表
# 可选值：
# True False
VerticalList=True
# 皮肤名称
SkinType=nord-dark

